{#
/**
 * @file gp.html.twig
 * Default theme implementation to present GP data.
 *
 * This template is used when viewing GP pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_gp()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('gp') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
